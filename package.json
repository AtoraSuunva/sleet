{
  "name": "sleetcord",
  "version": "1.5.1",
  "description": "An interaction-first command library for Discord.js",
  "license": "MIT",
  "author": "AtoraSuunva",
  "type": "module",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/ && prettier --check src/ && tsc --noEmit",
    "lint:fix": "eslint src/ --fix && prettier --write src/ && tsc --noEmit",
    "start:dev": "node --loader ts-node/esm -r dotenv/config test/index.ts",
    "prepublishOnly": "pnpm lint && pnpm build"
  },
  "dependencies": {
    "discord-api-types": "^0.37.84",
    "tseep": "^1.2.1"
  },
  "peerDependencies": {
    "discord.js": "^14.12.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "dotenv": "^16.4.5",
    "env-var": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "3.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
