{
  "name": "sleetcord",
  "version": "0.8.0",
  "description": "An interaction-first command library for Discord.js",
  "license": "MIT",
  "author": "AtoraSuunva",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/ && prettier --check src/ && tsc --noEmit",
    "lint:fix": "eslint src/ --fix && prettier --write src/ && tsc --noEmit",
    "start:dev": "node --loader ts-node/esm -r dotenv/config test/index.ts | pino-pretty",
    "prepublishOnly": "pnpm run lint && pnpm run build"
  },
  "dependencies": {
    "@discordjs/rest": "^1.6.0",
    "discord-api-types": "^0.37.36",
    "discord.js": "^14.8.0",
    "env-var": "^7.3.0",
    "pino": "^8.11.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "pino-pretty": "^10.0.0",
    "prettier": "2.8.5",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  }
}
