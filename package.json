{
  "name": "sleetcord",
  "version": "0.1.1",
  "description": "An interaction-first command library for Discord.js",
  "license": "MIT",
  "author": "AtoraSuunva",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "unbuild",
    "lint": "eslint src/ && prettier --check src/ && tsc --noEmit",
    "lint:fix": "eslint src/ --fix && prettier --write src/ && tsc --noEmit",
    "main": "node --loader ts-node/esm -r dotenv/config test/index.ts | pino-pretty",
    "prepublishOnly": "rimraf dist .tsbuildinfo && pnpm run build"
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "discord-api-types": "^0.37.0",
    "discord.js": "^14.1.2",
    "env-var": "^7.1.1",
    "pino": "^8.3.1",
    "undici": "^5.8.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.222",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "pino-pretty": "^8.1.0",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6"
  }
}
