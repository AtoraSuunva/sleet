{
  "name": "sleetcord",
  "version": "0.7.1",
  "description": "An interaction-first command library for Discord.js",
  "license": "MIT",
  "author": "AtoraSuunva",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "unbuild",
    "lint": "eslint src/ && prettier --check src/ && tsc --noEmit",
    "lint:fix": "eslint src/ --fix && prettier --write src/ && tsc --noEmit",
    "start:dev": "node --loader ts-node/esm -r dotenv/config test/index.ts | pino-pretty",
    "prepublishOnly": "rimraf dist .tsbuildinfo && pnpm run build"
  },
  "dependencies": {
    "@discordjs/rest": "^1.5.0",
    "discord-api-types": "^0.37.24",
    "discord.js": "^14.7.1",
    "env-var": "^7.3.0",
    "pino": "^8.8.0",
    "undici": "^5.14.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.24",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "pino-pretty": "^9.1.1",
    "prettier": "2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2"
  }
}
